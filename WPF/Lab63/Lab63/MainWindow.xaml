<Window x:Class="Lab63.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lab63"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525" Loaded="Window_Loaded">
    <Grid>
        <Button x:Name="button1" Content="new" HorizontalAlignment="Left" Margin="134,125,0,0" VerticalAlignment="Top" Width="75" Command="ApplicationCommands.New"/>
        <Button x:Name="button2" Content="new" HorizontalAlignment="Left" Margin="309,125,0,0" VerticalAlignment="Top" Width="75" Command="ApplicationCommands.New"/>
        <CheckBox x:Name="checkBox" Content="Command Active" IsChecked="True" HorizontalAlignment="Left" Margin="212,215,0,0" VerticalAlignment="Top"/>
        <Menu x:Name="menu" HorizontalAlignment="Stretch" Height="25" Margin="0" VerticalAlignment="Top" Width="Auto">
            <MenuItem Header="_File">
                <MenuItem Command="ApplicationCommands.New" />
            </MenuItem>
        </Menu>

    </Grid>
</Window>
